<h1>Listing photos</h1>

<table>
  <thead>
    <tr>
      <th>Uuid</th>
      <th>Md5</th>
      <th>Caption</th>
      <th>Adult content</th>
      <th>Url</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @photos.each do |photo| %>
      <tr>
        <td><%= photo.uuid %></td>
        <td><%= photo.md5 %></td>
        <td><%= photo.caption %></td>
        <td><%= photo.adult_content %></td>
        <td><%= photo.url %></td>
        <td><%= link_to 'Show', photo %></td>
        <td><%= link_to 'Edit', edit_photo_path(photo) %></td>
        <td><%= link_to 'Destroy', photo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Photo', new_photo_path %>
